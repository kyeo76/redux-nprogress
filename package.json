    "node-sass": "^3.8.0",
    "normalize.css": "^4.2.0",
    "nprogress": "^0.2.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.4.5",
    "react-sticky": "^5.0.5",
    "react-syntax-highlighter": "^2.2.0",
    "redux": "^3.5.2",
